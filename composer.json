{
    "name": "susonwaiba/php-varnish-cache",
    "description": "Varnish cache integration for PHP",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SusonWaiba\\PhpVarnishCache\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Suson Waiba",
            "email": "susonwaiba@gmail.com"
        }
